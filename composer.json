{
    "name": "gpslab/sitemap",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/gpslab/sitemap",
    "description": "sitemap.xml builder",
    "autoload": {
        "psr-4": {
            "GpsLab\\Component\\Sitemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GpsLab\\Component\\Sitemap\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "ext-zlib": "*",
        "ext-xmlwriter": "*",
        "psr/log": "~1.0",
        "phpunit/phpunit": "~7.5"
    },
    "suggest": {
        "ext-xmlwriter": "Allow use XMLWriter for render sitemap.xml",
        "ext-zlib": "Allow use compression for sitemap.xml"
    }
}
